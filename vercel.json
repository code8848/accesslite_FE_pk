{
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)\\.(js)$",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/assets/(.*)\\.(css)$",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}